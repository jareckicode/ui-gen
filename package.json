{
  "name": "@jareckicode/ui-gen",
  "version": "1.0.2",
  "description": "CLI generator for React components with TypeScript, Tailwind CSS, and testing setup",
  "main": "dist/cli.js",
  "bin": {
    "ui-gen": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "src/templates/"
  ],
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "typescript",
    "cli",
    "generator",
    "tailwind"
  ],
  "author": "Micha≈Ç Jarecki",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0"
  }
}
